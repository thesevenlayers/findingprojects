<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fp.be.contact.get_edit" pattern="/contact" methods="GET">
        <default key="_controller">FPBEBundle:Contact:getEdit</default>
    </route>

    <route id="fp.be.contact.post_edit_form" pattern="/contact" methods="POST">
        <default key="_controller">FPBEBundle:Contact:postEditForm</default>
    </route>


</routes>
